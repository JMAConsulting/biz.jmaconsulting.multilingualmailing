<?xml version="1.0"?>
<extension key="biz.jmaconsulting.multilingualmailing" type="module">
  <file>multilingualmailing</file>
  <name>MultiLingual Mailing Links</name>
  <description>This extension adds the ability to add a link to view email in a different language</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Edsel Lopez</author>
    <email>edsel.lopez@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-08-21</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>Currently only supports english and french</comments>
  <civix>
    <namespace>CRM/Multilingualmailing</namespace>
    <format>25.01.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CiviMix\Schema\Multilingualmailing\AutomaticUpgrader</upgrader>
  <mixins>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
